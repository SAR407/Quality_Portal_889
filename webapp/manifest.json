{
  "_version": "1.60.0",
  "sap.app": {
    "id": "qualityportal",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "fa7e0303-257d-472d-99dc-716563aa4893"
    },
    "dataSources": {
      "loginService": {
        "uri": "/sap/opu/odata/sap/ZCDS_QP_LOGIN_889_CDS/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      },
      "inspectionService": {
        "uri": "/sap/opu/odata/sap/ZCDS_QP_INSPECTION_889_CDS/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.14",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "qualityportal.i18n.i18n"
        }
      },
      "loginModel": {
        "dataSource": "loginService",
        "preload": true
      },
      "inspectionModel": {
        "dataSource": "inspectionService",
        "preload": true
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "qualityportal.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteLogin",
          "pattern": "",
          "target": "TargetLogin"
        },
        {
          "name": "RouteDashboard",
          "pattern": "dashboard",
          "target": "TargetDashboard"
        },
        {
          "name": "RouteInspectionLot",
          "pattern": "inspection-lot",
          "target": "TargetInspectionLot"
        },
        {
          "name": "RouteResultRecording",
          "pattern": "result-recording/{InspectionLot}",
          "target": "TargetResultRecording"
        },
        {
          "name": "RouteUsageDecision",
          "pattern": "usage-decision/{InspectionLot}",
          "target": "TargetUsageDecision"
        }
      ],
      "targets": {
        "TargetLogin": {
          "viewId": "Login",
          "viewName": "Login"
        },
        "TargetDashboard": {
          "viewId": "Dashboard",
          "viewName": "Dashboard"
        },
        "TargetInspectionLot": {
          "viewId": "InspectionLot",
          "viewName": "InspectionLot"
        },
        "TargetResultRecording": {
          "viewId": "ResultRecording",
          "viewName": "ResultRecording"
        },
        "TargetUsageDecision": {
          "viewId": "UsageDecision",
          "viewName": "UsageDecision"
        }
      }
    },
    "rootView": {
      "viewName": "qualityportal.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}